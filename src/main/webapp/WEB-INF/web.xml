  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterMainEvent</servlet-name>
  	<url-pattern>/RegisterMainEvent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterSubEvent</servlet-name>
  	<url-pattern>/RegisterSubEvent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
